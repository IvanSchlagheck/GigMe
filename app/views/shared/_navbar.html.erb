<div class="navbar">
  <div>
    <% unless current_page?(root_path) %>
      <div>
        <%= link_to "GigMe", root_path, class: "gigmelogo" %>
      </div>
    <% end %>
  </div>
  <div class="d-flex justify-content-between">
    <div class="navbar-items">
      <% unless current_user %>
          <%= link_to "Login", new_user_session_path, class: "signuplogin" %>
          <%= link_to "Sign up", new_user_registration_path, class: "signuplogin" %>
        <% end %>
        <% if current_user %>
          <%= link_to "Log out", destroy_user_session_path, method: :delete, class: "signuplogin" %>
          <%= link_to "My dashboard", dashboard_path, class: "signuplogin" %>
        <% end %>
      <div class="card-pink-line-navbar" data-aos="fade-left" data-aos-duration="1500"></div>
      <div class="card-green-line-navbar" data-aos="fade-left" data-aos-duration="2000"></div>
    </div>
  </div>
</div>
